<%- include("../../inclueds/head")  %>
<head>
<body>
<%- include("../../inclueds/uper_headerVendoer")  %> 
<!-- |||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
<%- include("../../inclueds/lowerVendoerNave")  %> 


    <!-- End Section -->
        <!-- Start Brands -->
        <div class="cars-brands-home">
            <div class="container">
                <h4>
                    <span class="title">اقسام المنتجات الخاصه بي</span>
                    <span class="float-right view-all"><a href="allMyCatigory">إظهار الكل</a> <i class="fas fa-angle-left"></i></span>
                </h4>

                <div class="row">
                    <% allMyCatigory.forEach(element => { %>
                        <div class="col-lg-2">
                            <div class="brand-box">
                                <a href="catigoryDetails/<%= element.id%>">
                                    <% if (!element.catigoryId) { %>
                                        <img style="height:100px;width:150px" src="/admin/asset/images/categoris_photo/<%= element.image.split("--")[0] %>" class="img-fluid" alt="0" />
                                    <% } else { %>
                                        <img style="height:100px;width:150px" src="/admin/asset/images/subCatigory_photo/<%= element.image.split("--")[0] %>" class="img-fluid" alt="0" />
                                    <% } %>
                                </a>
                            </div>
                            <h6 class="text-center"><%= element["name_" + defaultLanguage] %></h6>
                        </div>
                    <% }) %>

                </div>

            </div>
        </div>
        <!-- End Brands -->

        <!-- Start Section Products -->
        <div class="category-products">
            <div class="container">
                
                <div class="row">
                    <div class="col-12">
                        
                        <h5 class="title d-flex align-items-center justify-content-between">
                            <span>اخر <%=newerProducts.length%> منتجات قمت بادخالها (<%=newerProducts.length%>)</span>
                            <a class="h6" href="AllProductsPage">All Products</a>
                        </h5>

                        <br />

                        <div class="row">
                            
                            <% newerProducts.forEach(element => { %>
                                <div class="col-md-3 col-sm-6">
                                    <div class="product-grid3">
                                        <div class="product-image3">
                                            <a href="/productDetails/<%= element.id%>">
                                                <img style="height:290px" class="pic-1" src="/admin/asset/images/products_image/<%= element.productImage.split("--")[0] %>">
                                            </a>
                                        </div>
                                        <div class="product-content">
                                            <h3 class="title">
                                                <a href="productDetails/<%= element.id%>"><%= element["productName_" + defaultLanguage]%></a>
                                                <span class="float-right"><%= element.productState ? "جديد" : "قديم" %></span>
                                            </h3>
                                            <p style="width: 120px;overflow: hidden;white-space: nowrap;"><%= element["ProductOverview_" + defaultLanguage] %></p>
                                            <div>

                                                <% if (element.descount) { %>
                                                    <span class="price main-color font-weight-bold"><%= (element.price - ((element.descount / 100) * element.price)).toFixed(3) %> Eg</span>
                                                    <span style="text-decoration: line-through" class="price main-color font-weight-bold"><%= element.price %> Eg</span>
                                                <% } else {%>
                                                    <span class="price main-color font-weight-bold"><%= element.price %> Eg</span>
                                                <% } %>
                                                <span class="float-right">
                                                    <a href="vendoer_edit_product/<%=element.id%>"><i class="fa fa-fw fa-edit"></i></a>
                                                    <a href="#"><i class="far fa-fw fa-trash-alt text-danger"></i></a>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            <% }) %>

                            <div class="col-md-3 col-sm-6">
                                <div class="product-grid3">
                                    <div class="product-image3">
                                        <a href="vendoer_add_product">
                                            <img class="pic-1" src="/frontEnd/assets/img/add.svg" style="height:200px;padding:11px;" />
                                        </a>
                                    </div>
                                    
                                </div>
                            </div>

                            

                        </div><!-- End row -->

                        <br />
                    </div><!-- End col 9 -->

                   
                </div><!-- End Row -->

            </div>
        </div>
        <!-- End Section Products -->


<!-- |||||||||||||||||||||||||||||||||||||||||||||||||||||| -->
<%- include("../../inclueds/footer")  %>
<%- include("../../inclueds/footer_script")  %>
</body>
</head>