                                <!-- <h4 class="text-right"><a href="#"><i class="fa fa-edit"></i></a></h4> -->

                                <form enctype="multipart/form-data" action="" method="post">
                                    <div class="form-group">
                                        <label for="vendorFname">الاسم الاول </label>
                                        <input type="text" name="vendorFname" class="form-control" id="storeName" value="<%= vendoer.vendorFname %>" />
                                        <% if (validationError) { %>
                                            <% if (validationError.vendorFname) { %>
                                                <span class="text-danger"><%=validationError.vendorFname[0].msg %></span>
                                            <% } %>
                                        <% } %>
                                    </div>
                                    <div class="form-group">
                                        <label for="vendorLname">الاسم الثاني</label>
                                        <input type="text" name="vendorLname" class="form-control" id="vendorLname" value="<%= vendoer.vendorLname %>">
                                        <% if (validationError) { %>
                                            <% if (validationError.vendorLname) { %>
                                                <span class="text-danger"><%=validationError.vendorLname[0].msg %></span>
                                            <% } %>
                                        <% } %>
                                    </div>
                                    <div class="form-group">
                                        <label for="vendoerEmail">الايميل</label>
                                        <input type="text" name="vendoerEmail" class="form-control" id="vendoerEmail" value="<%= vendoer.vendoerEmail %>">
                                        <% if (validationError) { %>
                                            <% if (validationError.vendoerEmail) { %>
                                                <span class="text-danger"><%=validationError.vendoerEmail[0].msg %></span>
                                            <% } %>
                                        <% } %>
                                    </div>

                                    <div class="form-group">
                                        <label for="owner">اسم صاحب المتجر</label>
                                        <input type="text" readonly class="form-control" id="owner" value="<%= user.fName + " " + user.lName %>">
                                    </div>
                                    
                                    <div class="form-group">
                                        <label for="email">ايميل صاحب المتجر</label>
                                        <input type="text" readonly class="form-control" id="email" value="<%= user.email %>">
                                    </div>

                                    <div class="form-group">
                                        <label for="age">العمر</label>
                                        <input type="text" readonly class="form-control" id="age" value="<%= user.age %>">
                                    </div>

                                    <div class="form-group">
                                        <label for="addres">العنوان</label>
                                        <input type="text" readonly class="form-control" id="addres" value="<%= user.addres %>">
                                    </div>

                                    <div class="form-group">
                                        <label for="mobile">رقم الجوال</label>
                                        <input type="number" name="mobile" class="form-control" id="mobile" value="0<%= vendoer.mobile %>">
                                        <% if (validationError) { %>
                                            <% if (validationError.mobile) { %>
                                                <span class="text-danger"><%=validationError.mobile[0].msg %></span>
                                            <% } %>
                                        <% } %>
                                    </div>

                                    <div class="form-group">
                                        <label for="projectinput2"> ادخل الصوره الخاصه بالمتجر</label>
                                        <div class="custom-file">
                                            <input type="file" name="image" class="custom-file-input" id="inputGroupFile01">
                                            <input type="hidden" class="form-control" name="oldImage" id="oldImage" value="<%= vendoer.vendorImage %>">
                                            <label class="custom-file-label" for="inputGroupFile01">chose image </label>
                                        </div>
                                        <% if (validationError) { %>
                                            <% if (validationError.image) { %>
                                                <span class="text-danger"><%=validationError.image[0].msg %></span>
                                            <% } %>
                                        <% } %>
                                    </div>
                                    
                                    <div class="form-group">
                                        <label for="projectinput2"> ادخل السجل التجاري الخاص بك اذا اردت</label>
                                        <div class="custom-file">
                                            <input type="file" name="commercFile" class="custom-file-input" id="inputGroupFile02">
                                            <input type="hidden" class="form-control" name="oldCommerc" id="oldCommerc" value="<%= vendoer.commercFile %>">
                                            <label class="custom-file-label" for="inputGroupFile02">chose file </label>
                                        </div>
                                        <% if (validationError) { %>
                                            <% if (validationError.commercFile) { %>
                                                <span class="text-danger"><%=validationError.commercFile[0].msg %></span>
                                            <% } %>
                                        <% } %>
                                    </div>
           
                                    <div class="form-group text-center">
                                        <button class="btn-primary btn">حفظ</button>
                                    </div>



                                </form>

                            </div>
                            <div class="tab-pane fade" id="v-pills-profile" role="tabpanel" aria-labelledby="v-pills-profile-tab">

                                <div class="text-center">
                                    <span class="btn btn-primary" data-toggle="modal" data-target="#exampleModal">إضافة تخصص</span>
                                </div>

                                <br />

