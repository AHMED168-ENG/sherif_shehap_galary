        <div class="content">
            <div class="container">
                
                <div>
                    
                    <p><img src="/admin/asset/images/users_photo/<%= vendoer.vendorImage.split("--")[0] %>" /></p>
                    <h6><%= vendoer.vendorFname + " " + vendoer.vendorLname %></h6>
                    <p style="margin-bottom:10px">
                        <i class="fas fa-map-marker-alt"></i>
                        <span><%= user.addres %></span>
                    </p>
                    <p style="margin-bottom:10px">
                        <a href="/admin/asset/images/users_photo/<%= vendoer.commercFile.split("--")[0] %>">السجل التجاري</a>
                    </p>
                    <p>
                        <i class="far fa-star"></i>
                        <span>3.89</span> <br />
                        <span>قيم 536 شخصًا</span>
                    </p>
                </div>

            </div>

        </div>
    </div>



    <div class="page pt-5">
